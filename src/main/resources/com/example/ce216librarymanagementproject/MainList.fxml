                                <TableColumn fx:id="titleCol" prefWidth="75.0" text="TITLE" />
                                <TableColumn fx:id="subtitleCol" prefWidth="75.0" text="SUBTITLE" />
                                <TableColumn fx:id="translatorCol" prefWidth="75.0" text="TRANSLATOR" />
