<AnchorPane prefHeight="605.0" prefWidth="986.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ce216librarymanagementproject.MainController">
    <children>
        <VBox prefHeight="613.0" prefWidth="986.0">
